    "def solution(array, commands):\n",
    "    answer=[]\n",
    "    for command in commands :\n",
    "        slices = array[command[0]-1:command[1]]\n",
    "        slices.sort()\n",
    "        answer.append(slices[command[2]-1])\n",
    "    return answer"
   ]
